<div class="container">
{{#each Parts}}
    <div class="card mt-3">
        <h5 class="card-header">{{this.status}}</h5>
        <div class="card-body">
            <h5 class="card-title">{{this.part}}</h5>
            <p class="card-text">{{this.link}}</p>
            <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>
{{/each}}
</div>